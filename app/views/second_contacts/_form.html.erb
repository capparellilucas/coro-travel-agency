<%= simple_form_for(second_contact) do |f| %>
  <%= f.input :second_contact_name %>
  <%= f.input :second_contact_phone %>
  <%= f.input :second_contact_mail %>
  <%= f.input :second_contact_school %>
  <%= f.input :second_contact_grade %>
  <%= f.input :second_contact_division %>
  <%= f.input :second_contact_shift %>
  <%= f.input :second_contact_school_location %>
  <%= f.submit %>
<% end %>

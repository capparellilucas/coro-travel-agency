<%= simple_form_for(contact) do |f| %>
  <%= f.input :contact_name %>
  <%= f.input :contact_phone %>
  <%= f.input :contact_mail %>
  <%= f.input :contact_school %>
  <%= f.input :contact_grade %>
  <%= f.input :contact_division %>
  <%= f.input :contact_shift %>
  <%= f.input :contact_school_location %>
  <%= f.submit %>
<% end %>
